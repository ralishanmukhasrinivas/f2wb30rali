extends layout
block content
  h1= title
  - var image_names = ["ss1", "ss2", "ss3", "ss4", "ss5"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.jpg`, alt="cricket i like", height="200px", width="200px")
  - }